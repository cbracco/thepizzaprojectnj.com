{
    "name": "thepizzaprojectnj.com",
    "version": "1.0.0",
    "description": "The source code for the thepizzaprojectnj.com website.",
    "scripts": {
        "build": "NODE_ENV=production eleventy",
        "clean": "rimraf _site",
        "start": "run-p dev:*",
        "dev:eleventy": "eleventy --serve --quiet",
        "dev:css": "postcss src/styles/index.css -o _site/styles/index.min.css --watch --verbose",
        "debug": "DEBUG=Eleventy* eleventy",
        "debugstart": "DEBUG=Eleventy* eleventy --serve --quiet",
        "benchmark": "DEBUG=Eleventy:Benchmark* eleventy",
        "prepare": "husky install",
        "lint:content": "prettier --check .",
        "lint:styles": "stylelint **/*.css",
        "lint:scripts": "eslint --ext **/*.js",
        "lint": "run-s lint:*",
        "format:content": "prettier --write .",
        "format:styles": "stylelint src/styles/**/*.css --fix",
        "format:scripts": "eslint --ext **/*.js --fix",
        "format": "run-s format:*"
    },
    "browserslist": [
        "defaults and fully supports es6-module",
        "maintained node versions"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cbracco/thepizzaprojectnj.com.git"
    },
    "author": {
        "name": "Chris Bracco",
        "email": "work@chrisbracco.com",
        "url": "https://chrisbracco.com/"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "bugs": {
        "url": "https://github.com/cbracco/thepizzaprojectnj.com/issues"
    },
    "homepage": "https://github.com/cbracco/thepizzaprojectnj.com#readme",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "@11ty/eleventy-img": "^3.1.1",
        "@11ty/eleventy-navigation": "^0.3.5",
        "@11ty/eleventy-plugin-bundle": "^1.0.4",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
        "@11ty/eleventy-server-browsersync": "^1.0.2",
        "cssnano": "^6.0.2",
        "dotenv": "^16.3.1",
        "eleventy-auto-cache-buster": "^0.3.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "html-minifier": "^4.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.1.0",
        "luxon": "^3.3.0",
        "markdown-it-anchor": "^8.6.7",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.32",
        "postcss-cli": "^11.0.0",
        "postcss-import": "^16.0.0",
        "postcss-load-config": "^5.0.2",
        "postcss-preset-env": "^9.3.0",
        "prettier": "^3.1.0",
        "rimraf": "^5.0.5",
        "stylelint": "^15.11.0",
        "stylelint-config-standard": "^34.0.0"
    }
}
